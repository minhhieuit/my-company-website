@model MVC_Thehegeo.Models.BlogModels.TeamInfo

<div class="row">
    <div class="col-lg-12">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>All form elements <small>With custom checbox and radion elements.</small></h5>
                <div class="ibox-tools">
                    <a class="collapse-link">
                        <i class="fa fa-chevron-up"></i>
                    </a>
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        <i class="fa fa-wrench"></i>
                    </a>
                    <ul class="dropdown-menu dropdown-user">
                        <li><a href="#">Config option 1</a>
                        </li>
                        <li><a href="#">Config option 2</a>
                        </li>
                    </ul>
                    <a class="close-link">
                        <i class="fa fa-times"></i>
                    </a>
                </div>
            </div>
            <div class="ibox-content">
                <form asp-action="Create" class="form-horizontal" enctype="multipart/form-data">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label">FullName-VI</label>
                        <div class="col-sm-10">
                            <input asp-for="FullNameVI" type="text" class="form-control">
                            <span asp-validation-for="FullNameVI" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label">FullName-EN</label>
                        <div class="col-sm-10">
                            <input asp-for="FullNameEN" type="text" class="form-control">
                            <span asp-validation-for="FullNameEN" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label">Function-VI</label>
                        <div class="col-sm-10">
                            <input asp-for="FunctionVI" type="text" class="form-control"></input>
                            <span asp-validation-for="FunctionVI" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label">Function-EN</label>
                        <div class="col-sm-10">
                            <input asp-for="FunctionEN" type="text" class="form-control"></input>
                            <span asp-validation-for="FunctionEN" class="text-danger"></span>
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="col-sm-2 control-label">Avatar</label>
                        <div class="col-sm-10">
                            <input asp-for="Avatar" type="file" class="form-control">
                            <span asp-validation-for="Avatar" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label">Email</label>
                        <div class="col-sm-10">
                            <input asp-for="Email" type="text" class="form-control"></input>
                            <span asp-validation-for="Email" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label">Facebook Link</label>
                        <div class="col-sm-10">
                            <input asp-for="FacebookLink" type="text" class="form-control"></input>
                            <span asp-validation-for="FacebookLink" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label">GooglePlusLink</label>
                        <div class="col-sm-10">
                            <input asp-for="GooglePlusLink" type="text" class="form-control"></input>
                            <span asp-validation-for="GooglePlusLink" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label">TwitterLink</label>
                        <div class="col-sm-10">
                            <input asp-for="TwitterLink" type="text" class="form-control"></input>
                            <span asp-validation-for="TwitterLink" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="hr-line-dashed"></div>
                    <div class="form-group">
                        <div class="col-sm-4 col-sm-offset-2">
                            <button class="btn btn-white" type="button">Cancel</button>
                            <button class="btn btn-primary" type="submit">Save changes</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
@section Scripts{
    <!-- SUMMERNOTE -->
    <script src="~/Admin/js/plugins/summernote/summernote.min.js"></script>

    <script>
        $(document).ready(function(){

            $('.summernote').summernote();

       });
    </script>
}